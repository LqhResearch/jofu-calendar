{
    "name": "@jofu/calendar",
    "version": "0.0.4",
    "description": "A lightweight and customizable React component for selecting dates. Built with Tailwind CSS, it offers a seamless user experience with modern UI design and easy integration.",
    "keywords": [
        "jofu",
        "calendar",
        "datepicker",
        "timepicker",
        "datetimepicker"
    ],
    "homepage": "https://github.com/LqhResearch/jofu-calendar#readme",
    "author": "LqhResearch <lqhung.max@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LqhResearch/jofu-calendar.git"
    },
    "bugs": {
        "url": "https://github.com/LqhResearch/jofu-calendar/issues"
    },
    "engines": {
        "node": ">=14"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "files": [
        "dist/index.js",
        "dist/index.mjs",
        "dist/index.d.ts",
        "dist/jofu-calendar.min.css"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./style": {
            "import": "./dist/jofu-calendar.min.css"
        }
    },
    "browserslist": [
        "defaults"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --minify && npm run tailwind",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "pub": "npm run build && npm publish --access public",
        "tailwind": "npx @tailwindcss/cli -i ./src/style.css -o ./dist/jofu-calendar.min.css --minify",
        "format": "prettier --write .",
        "storybook": "storybook dev -p 6006"
    },
    "peerDependencies": {
        "react": ">=17.0.0 <20.0.0",
        "react-dom": ">=17.0.0 <20.0.0",
        "typescript": "^5.6.0"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^8.4.7",
        "@storybook/addon-interactions": "^8.4.7",
        "@storybook/blocks": "^8.4.7",
        "@storybook/react-vite": "^8.4.7",
        "@storybook/test": "^8.4.7",
        "@tailwindcss/cli": "^4.0.8",
        "@tailwindcss/postcss": "^4.0.8",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.5.3",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "storybook": "^8.4.7",
        "tailwindcss": "^4.0.8",
        "tsup": "^8.3.5"
    },
    "sideEffects": false
}
